{
  "name": "simp-simp_docker",
  "version": "0.1.1",
  "author": "SIMP Team",
  "summary": "A SIMP helper module for puppetlabs/docker",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_docker",
  "project_page": "https://github.com/simp/pupmod-simp-simp_docker",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "docker",
    "containers",
    "systemd"
  ],
  "dependencies": [
    {
      "name": "herculesteam/augeasproviders_sysctl",
      "version_requirement": ">= 2.2.0 < 3.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.3.1 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "puppetlabs/docker",
      "version_requirement": ">= 1.1.0 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.6.0 < 6.0.0"
    }
  ],
  "data_provider": "hiera"
}
